<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".scoreboard">


    <TextView
        android:id="@+id/pos1"
        android:layout_width="410dp"
        android:layout_height="109dp"
        android:layout_marginTop="116dp"
        android:layout_marginBottom="1dp"
        android:fontFamily="@font/contb"
        android:text="Grand Loser!"
        android:textSize="50sp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/pos2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/pos4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fontFamily="@font/contb"
        android:text="Loser!"
        android:textSize="20sp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/pos5"
        app:layout_constraintEnd_toEndOf="@+id/pos5"
        app:layout_constraintStart_toStartOf="@+id/pos5"
        app:layout_constraintTop_toBottomOf="@+id/pos3" />

    <TextView
        android:id="@+id/pos5"
        android:layout_width="200dp"
        android:layout_height="0dp"
        android:fontFamily="@font/contb"
        android:text="Loser!"
        android:textSize="20sp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/pos6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pos4" />

    <TextView
        android:id="@+id/pos6"
        android:layout_width="106dp"
        android:layout_height="0dp"
        android:fontFamily="@font/contb"
        android:text="Loser!"
        android:textSize="15sp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/pos7"
        app:layout_constraintEnd_toEndOf="@+id/pos5"
        app:layout_constraintStart_toStartOf="@+id/pos5"
        app:layout_constraintTop_toBottomOf="@+id/pos5" />

    <TextView
        android:id="@+id/pos7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fontFamily="@font/contb"
        android:text="Loser!"
        android:textSize="15sp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/pos8"
        app:layout_constraintEnd_toEndOf="@+id/pos6"
        app:layout_constraintStart_toStartOf="@+id/pos6"
        app:layout_constraintTop_toBottomOf="@+id/pos6" />

    <TextView
        android:id="@+id/pos8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fontFamily="@font/contb"
        android:text="Loser!"
        android:textSize="15sp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/pos9"
        app:layout_constraintEnd_toEndOf="@+id/pos7"
        app:layout_constraintStart_toStartOf="@+id/pos7"
        app:layout_constraintTop_toBottomOf="@+id/pos7" />

    <TextView
        android:id="@+id/pos9"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fontFamily="@font/contb"
        android:text="Loser!"
        android:textSize="15sp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/pos10"
        app:layout_constraintEnd_toEndOf="@+id/pos8"
        app:layout_constraintStart_toStartOf="@+id/pos8"
        app:layout_constraintTop_toBottomOf="@+id/pos8" />

    <TextView
        android:id="@+id/pos10"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="109dp"
        android:fontFamily="@font/contb"
        android:text="Loser!"
        android:textSize="15sp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/pos9"
        app:layout_constraintStart_toStartOf="@+id/pos9"
        app:layout_constraintTop_toBottomOf="@+id/pos9" />

    <TextView
        android:id="@+id/pos3"
        android:layout_width="410dp"
        android:layout_height="51dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:fontFamily="@font/contb"
        android:text="Loser!"
        android:textSize="35sp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/pos4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pos2" />

    <TextView
        android:id="@+id/pos2"
        android:layout_width="409dp"
        android:layout_height="73dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:fontFamily="@font/contb"
        android:text="Loser"
        android:textSize="40sp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/pos3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pos1" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_marginTop="20dp"

        android:src="@drawable/literallynot"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="406dp"
        android:layout_height="83dp"
        android:layout_marginEnd="3dp"
        android:src="@drawable/greenglass"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.454"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/reportbutton"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="236dp"
        android:layout_marginBottom="20dp"
        android:background="@android:color/transparent"
        android:maxHeight="40dp"
        android:scaleType="fitCenter"
        android:src="@drawable/bar_graph"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/homebutton" />

    <ImageButton
        android:id="@+id/homebutton"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"
        android:src="@drawable/house"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView" />


    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="56dp"
        android:layout_height="52dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="5dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"
        android:src="@drawable/trophy_selected"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/logout_button"
        app:layout_constraintStart_toEndOf="@+id/reportbutton"
        app:layout_constraintTop_toTopOf="@+id/imageView" />

    <ImageButton
        android:id="@+id/logout_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="5dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"
        android:src="@drawable/acc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView" />

</androidx.constraintlayout.widget.ConstraintLayout>